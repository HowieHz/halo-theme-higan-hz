    "typescript": "^5.6.3",
    "unplugin-fonts": "^1.1.1",
    "vite": "^3.2.11",
    "vite-plugin-purge-icons": "^0.10.0"
  },
  "dependencies": {
    "@iconify/iconify": "^2.2.1",
