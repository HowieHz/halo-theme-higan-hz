{
  "scripts": {
    "dev": "pnpm run \"/^watch:.*/\"",
    "watch:vite": "vite build --watch",
    "watch:stylus": "stylus ./src/assets/colors/theme.styl --out ./tmp/styles/ --watch",
    "watch:unocss": "unocss -w",
    "build-postcss": "postcss -i ./src/assets/tailwind.css -o ./tmp/styles/tailwind.css",
    "build": "set NODE_ENV=777 && stylus ./src/assets/colors/theme.styl --out ./tmp/styles/ --compress && tsc && vite build && unocss -m",
    "lint": "eslint ./src",
    "neo-format": "prettier --write \"./src/**/*.{js,ts,css,json,ml,yaml,html}\" \"./templates/**/*.html\" && stylus-supremacy format src/**/*.styl --options .vscode/settings.json -r",
    "format": "prettier --write \"./*.js\" \"./src/**/*.{js,ts,css,json,ml,yaml,html}\" \"./templates/**/*.html\"",
    "prepare": "husky"
  },
  "type": "module",
  "keywords": [
    "halo",
    "howiehz",
    "halo-theme",
    "halo-theme-higan",
    "halo-theme-higan-hz"
  ],
  "homepage": "https://github.com/howiehz/halo-theme-higan-hz#readme",
  "bugs": {
    "url": "https://github.com/howiehz/halo-theme-higan-hz/issues"
  },
  "author": {
    "name": "HowieHz",
    "url": "https://github.com/howiehz"
  },
  "maintainers": [
