    "type": "git"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@iconify/json": "^2.2.218",
    "@types/alpinejs": "^3.7.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "16",
    "@unocss/preset-icons": "^0.60.4",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.2",
    "daisyui": "^4.12.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
